{
  "version": 2,
  "builds": [
    {
      "src": "api/[...all].ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/[...all].ts"
    }
  ],
  "env": {
    "DATABASE_URL": "@postgres_prisma_url",
    "DIRECT_URL": "@postgres_url_non_pooling"
  }
}